+		cap_rights_init(&rights, CAP_WRITE, CAP_FSTAT, CAP_FCHOWN, CAP_FCHMOD, CAP_FUTIMES);
+		if (cap_rights_limit(pair->dest_fd, &rights) < 0 && errno != ENOSYS){
