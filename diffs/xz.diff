+			exit(E_ERROR);
+		}
+	}
+
+	if(pair->src_fd != -1 ){
+		cap_rights_init(&rights, CAP_READ, CAP_SEEK);
+		if (cap_rights_limit(pair->src_fd, &rights) < 0 && errno != ENOSYS){
+			message_error("%s: %s", pair->src_name, strerror(errno));
+			exit(E_ERROR);
