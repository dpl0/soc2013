# $FreeBSD: soc2013/dpl/head/share/skel/dot.mailrc 50516 1999-08-28 00:22:10Z peter $
#
# .mailrc - mail resources
#
# see also mail(1)
#

set append ask autoprint 
set indentprefix="> "
set PAGER=more
set EDITOR=vi
set VISUAL=vi
set folder=Mail
retain bcc cc date from subject to

# include your private mail aliases
source ~/.mail_aliases
